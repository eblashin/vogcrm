	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>Список.Ref</Field>
				<Field>Список.Клиент</Field>
				<Field>Список.КонтактноеЛицо</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ВЫБОР
		КОГДА ДокументCRM_Сделка.ЕстьЗаказыПокупателя 
			ТОГДА
				ВЫБОР
					КОГДА ДокументCRM_Сделка.СуммаОтгрузокПоСделке &gt;= ДокументCRM_Сделка.СуммаСделки
						ТОГДА 0
					КОГДА ДокументCRM_Сделка.СуммаОтгрузокПоСделке &lt; ДокументCRM_Сделка.СуммаСделки И ДокументCRM_Сделка.СуммаОтгрузокПоСделке &gt; 0
						ТОГДА
							ВЫБОР
								КОГДА ДокументCRM_Сделка.ДатаЗавершения = &amp;АктуальнаяДатаСеанса
									ТОГДА 3
								КОГДА ДокументCRM_Сделка.ДатаЗавершения &lt; &amp;АктуальнаяДатаСеанса
									ТОГДА 5
								ИНАЧЕ 1
							КОНЕЦ
					ИНАЧЕ
						ВЫБОР
							КОГДА ДокументCRM_Сделка.ДатаЗавершения = &amp;АктуальнаяДатаСеанса
								ТОГДА 4
							КОГДА ДокументCRM_Сделка.ДатаЗавершения &lt; &amp;АктуальнаяДатаСеанса
								ТОГДА 6
							ИНАЧЕ 2
						КОНЕЦ
					КОНЕЦ
		ИНАЧЕ 7
	КОНЕЦ КАК НомерКартинкиОтгрузки,
	ВЫБОР
		КОГДА ДокументCRM_Сделка.ЕстьЗаказыПокупателя 
			ТОГДА
				ВЫБОР
					КОГДА ДокументCRM_Сделка.СуммаОплатПоСделке &gt;= ДокументCRM_Сделка.СуммаСделки
						ТОГДА 0
					КОГДА ДокументCRM_Сделка.СуммаОплатПоСделке &lt; ДокументCRM_Сделка.СуммаСделки И ДокументCRM_Сделка.СуммаОплатПоСделке &gt; 0
						ТОГДА
							ВЫБОР
								КОГДА ДокументCRM_Сделка.ДатаЗавершения = &amp;АктуальнаяДатаСеанса
									ТОГДА 3
								КОГДА ДокументCRM_Сделка.ДатаЗавершения &lt; &amp;АктуальнаяДатаСеанса
									ТОГДА 5
								ИНАЧЕ 1
							КОНЕЦ
					ИНАЧЕ
						ВЫБОР
							КОГДА ДокументCRM_Сделка.ДатаЗавершения = &amp;АктуальнаяДатаСеанса
								ТОГДА 4
							КОГДА ДокументCRM_Сделка.ДатаЗавершения &lt; &amp;АктуальнаяДатаСеанса
								ТОГДА 6
							ИНАЧЕ 2
						КОНЕЦ
					КОНЕЦ
		ИНАЧЕ 7
	КОНЕЦ КАК НомерКартинкиОплаты,
	ДокументCRM_Сделка.Ссылка,
	ДокументCRM_Сделка.ПометкаУдаления,
	ДокументCRM_Сделка.Номер,
	ДокументCRM_Сделка.ДатаСтарта КАК ДатаНачала,
	ДокументCRM_Сделка.Проведен,
	ДокументCRM_Сделка.ДатаЗавершения КАК Срок,
	ДокументCRM_Сделка.Договор,
	ДокументCRM_Сделка.Описание,
	ДокументCRM_Сделка.Клиент,
    ДокументCRM_Сделка.Клиент.CRM_ОсновноеКонтактноеЛицо КАК КонтактноеЛицо,
	ДокументCRM_Сделка.Организация,
	ДокументCRM_Сделка.ПричинаОтказа,
	ВЫРАЗИТЬ(ДокументCRM_Сделка.Статус КАК СТРОКА(99)) КАК Состояние,
	ДокументCRM_Сделка.Ответственный,
	ДокументCRM_Сделка.Автор,
	ДокументCRM_Сделка.СуммаПотенциал КАК Потенциал,
	ВЫБОР
		КОГДА ДокументCRM_Сделка.Важность = ЗНАЧЕНИЕ(Перечисление.ВариантыВажностиВзаимодействия.Низкая)
			ТОГДА 0
		КОГДА ДокументCRM_Сделка.Важность = ЗНАЧЕНИЕ(Перечисление.ВариантыВажностиВзаимодействия.Высокая)
			ТОГДА 2
		ИНАЧЕ 1
	КОНЕЦ КАК Важность,

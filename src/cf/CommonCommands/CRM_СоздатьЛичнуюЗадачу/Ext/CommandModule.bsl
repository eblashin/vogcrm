	Если ТипЗнч(ПараметрКоманды) = Тип("ДокументСсылка.ЭлектронноеПисьмоИсходящее") Тогда
		Текст = "ru = 'Принять к исполнению можно только входящее письмо!'";
		ПоказатьПредупреждение(, НСтр(Текст),5);

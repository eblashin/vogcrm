		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
    ВЫБОР
        КОГДА План.ДатаНачала &lt; &amp;ДатаНачала
            ТОГДА &amp;ДатаНачала
        ИНАЧЕ План.ДатаНачала
    КОНЕЦ КАК ДатаНачала,
    ВЫБОР
        КОГДА План.ДатаКонца &gt; &amp;ДатаКонца
            ТОГДА &amp;ДатаКонца
        ИНАЧЕ План.ДатаКонца
    КОНЕЦ КАК ДатаКонца,
    ВЫРАЗИТЬ(ВЫБОР
            КОГДА РАЗНОСТЬДАТ(План.ДатаНачала, План.ДатаКонца, МИНУТА) &lt;= 0
                ТОГДА План.Трудоемкость
            ИНАЧЕ План.Трудоемкость - ВЫБОР
                    КОГДА План.ДатаНачала &lt; &amp;ДатаНачала
                        ТОГДА План.Трудоемкость * (РАЗНОСТЬДАТ(План.ДатаНачала, &amp;ДатаНачала, МИНУТА) / РАЗНОСТЬДАТ(План.ДатаНачала, План.ДатаКонца, МИНУТА))
                    ИНАЧЕ 0
                КОНЕЦ - ВЫБОР
                    КОГДА План.ДатаКонца &gt; &amp;ДатаКонца
                            И &amp;ДатаКонца &lt;&gt; ДАТАВРЕМЯ(1, 1, 1)
                        ТОГДА План.Трудоемкость * (РАЗНОСТЬДАТ(&amp;ДатаКонца, План.ДатаКонца, МИНУТА) / РАЗНОСТЬДАТ(План.ДатаНачала, План.ДатаКонца, МИНУТА))
                    ИНАЧЕ 0
                КОНЕЦ
        КОНЕЦ КАК ЧИСЛО(15, 1)) КАК Трудоемкость,
    План.Исполнитель КАК Пользователь,
    План.Проект КАК Проект,
    План.Этап КАК Этап
ПОМЕСТИТЬ План
ИЗ
    РегистрСведений.CRM_ПланыПроектов КАК План
ГДЕ
    (&amp;ДатаНачала = ДАТАВРЕМЯ(1, 1, 1)
            ИЛИ План.ДатаКонца &gt;= &amp;ДатаНачала)
    И (&amp;ДатаКонца = ДАТАВРЕМЯ(1, 1, 1)
            ИЛИ План.ДатаНачала &lt;= &amp;ДатаКонца)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
    Пользователи.Ссылка КАК Пользователь,
    ВЫБОР
        КОГДА ЕСТЬNULL(НастройкиПользователей1.Значение, ДАТАВРЕМЯ(1, 1, 1)) = ДАТАВРЕМЯ(1, 1, 1)
                ИЛИ ЕСТЬNULL(НастройкиПользователей2.Значение, ДАТАВРЕМЯ(1, 1, 1)) = ДАТАВРЕМЯ(1, 1, 1)
            ТОГДА 8
        ИНАЧЕ РАЗНОСТЬДАТ(ВЫРАЗИТЬ(НастройкиПользователей1.Значение КАК ДАТА), ВЫРАЗИТЬ(НастройкиПользователей2.Значение КАК ДАТА), ЧАС) - ВЫБОР
                КОГДА ЕСТЬNULL(НастройкиПользователей3.Значение, ДАТАВРЕМЯ(1, 1, 1)) = ДАТАВРЕМЯ(1, 1, 1)
                        ИЛИ ЕСТЬNULL(НастройкиПользователей4.Значение, ДАТАВРЕМЯ(1, 1, 1)) = ДАТАВРЕМЯ(1, 1, 1)
                    ТОГДА 0
                ИНАЧЕ РАЗНОСТЬДАТ(ВЫРАЗИТЬ(НастройкиПользователей3.Значение КАК ДАТА), ВЫРАЗИТЬ(НастройкиПользователей4.Значение КАК ДАТА), ЧАС)
            КОНЕЦ
    КОНЕЦ КАК РЧасовВДне
ПОМЕСТИТЬ РабочиеЧасыПользователей
ИЗ
    Справочник.Пользователи КАК Пользователи
        ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.CRM_НастройкиПользователей КАК НастройкиПользователей1
        ПО (НастройкиПользователей1.Пользователь = Пользователи.Ссылка)
            И (НастройкиПользователей1.Настройка = ЗНАЧЕНИЕ(ПланВидовХарактеристик.CRM_НастройкиПользователей.ВремяНачалаРабочегоДня))
        ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.CRM_НастройкиПользователей КАК НастройкиПользователей2
        ПО (НастройкиПользователей2.Пользователь = Пользователи.Ссылка)
            И (НастройкиПользователей2.Настройка = ЗНАЧЕНИЕ(ПланВидовХарактеристик.CRM_НастройкиПользователей.ВремяОкончанияРабочегоДня))
        ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.CRM_НастройкиПользователей КАК НастройкиПользователей3
        ПО (НастройкиПользователей3.Пользователь = Пользователи.Ссылка)
            И (НастройкиПользователей3.Настройка = ЗНАЧЕНИЕ(ПланВидовХарактеристик.CRM_НастройкиПользователей.ВремяНачалаОбеденногоПерерыва))
        ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.CRM_НастройкиПользователей КАК НастройкиПользователей4
        ПО (НастройкиПользователей4.Пользователь = Пользователи.Ссылка)
            И (НастройкиПользователей4.Настройка = ЗНАЧЕНИЕ(ПланВидовХарактеристик.CRM_НастройкиПользователей.ВремяОкончанияОбеденногоПерерыва))
ГДЕ
    Пользователи.Ссылка В
            (ВЫБРАТЬ РАЗЛИЧНЫЕ
                План.Пользователь
            ИЗ
                План)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
    ВложенныйЗапросПлан.Период,
    ВложенныйЗапросПлан.Пользователь,
    ВложенныйЗапросПлан.Проект,
    ВложенныйЗапросПлан.Этап,
    ВложенныйЗапросПлан.ЧасовПлан,
    ВложенныйЗапросПлан.ЧасовФакт,
    ВложенныйЗапросПлан.ЧасовРасхождение
ПОМЕСТИТЬ ПланФактСводно
ИЗ
    (ВЫБРАТЬ
        План.ДатаКонца КАК Период,
        План.Пользователь КАК Пользователь,
        План.Проект КАК Проект,
        План.Этап КАК Этап,
        СУММА(План.Трудоемкость) КАК ЧасовПлан,
        СУММА(0) КАК ЧасовФакт,
        СУММА(-План.Трудоемкость) КАК ЧасовРасхождение
    ИЗ
        План КАК План
    
    СГРУППИРОВАТЬ ПО
        План.ДатаКонца,
        План.Пользователь,
        План.Проект,
        План.Этап) КАК ВложенныйЗапросПлан

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
    ВложенныйЗапросФакт.Период,
    ВложенныйЗапросФакт.Пользователь,
    ВложенныйЗапросФакт.Проект,
    ВложенныйЗапросФакт.Этап,
    ВложенныйЗапросФакт.ЧасовПлан,
    ВложенныйЗапросФакт.ЧасовФакт,
    ВложенныйЗапросФакт.ЧасовРасхождение
ИЗ
    (ВЫБРАТЬ
        Факт.ДатаРабот КАК Период,
        Факт.Сотрудник КАК Пользователь,
        Факт.Проект КАК Проект,
        Факт.Этап КАК Этап,
        0 КАК ЧасовПлан,
        Факт.ЧасовОборот КАК ЧасовФакт,
        Факт.ЧасовОборот КАК ЧасовРасхождение
    ИЗ
        РегистрНакопления.CRM_РабочееВремя.Обороты(НАЧАЛОПЕРИОДА(&amp;ДатаНачала, ДЕНЬ), КОНЕЦПЕРИОДА(&amp;ДатаКонца, ДЕНЬ), Регистратор, ) КАК Факт) КАК ВложенныйЗапросФакт
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
    ПланФактСводно.Период,
    ПланФактСводно.Пользователь,
    ПланФактСводно.Проект,
    ПланФактСводно.Этап,
    ПланФактСводно.ЧасовПлан,
    ПланФактСводно.ЧасовФакт,
    ПланФактСводно.ЧасовРасхождение,
    ПланФактСводно.ЧасовПлан / ЕСТЬNULL(РабочиеЧасыПользователей.РЧасовВДне, 8) КАК ДнейПлан,
    ПланФактСводно.ЧасовФакт / ЕСТЬNULL(РабочиеЧасыПользователей.РЧасовВДне, 8) КАК ДнейФакт,
    ПланФактСводно.ЧасовРасхождение / ЕСТЬNULL(РабочиеЧасыПользователей.РЧасовВДне, 8) КАК ДнейРасхождение,
    ВЫБОР
        КОГДА ПланФактСводно.ЧасовПлан = 0
            ТОГДА ВЫБОР
                    КОГДА ПланФактСводно.ЧасовФакт = 0
                        ТОГДА 0
                    ИНАЧЕ 100
                КОНЕЦ
        ИНАЧЕ ПланФактСводно.ЧасовРасхождение / ПланФактСводно.ЧасовПлан * 100
    КОНЕЦ КАК ПроцентовРасхождение
ПОМЕСТИТЬ ПланФакт
ИЗ
    ПланФактСводно КАК ПланФактСводно
        ЛЕВОЕ СОЕДИНЕНИЕ РабочиеЧасыПользователей КАК РабочиеЧасыПользователей
        ПО ПланФактСводно.Пользователь = РабочиеЧасыПользователей.Пользователь
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
    НАЧАЛОПЕРИОДА(ПланФакт.Период, НЕДЕЛЯ) КАК ПериодНеделя,
    НАЧАЛОПЕРИОДА(ПланФакт.Период, МЕСЯЦ) КАК ПериодМесяц,
    НАЧАЛОПЕРИОДА(ПланФакт.Период, КВАРТАЛ) КАК ПериодКвартал,
    НАЧАЛОПЕРИОДА(ПланФакт.Период, ГОД) КАК ПериодГод,

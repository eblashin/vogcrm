	И (КОНЕЦПЕРИОДА(вогРейтинги.ДатаОкончания, ДЕНЬ) &gt;= &amp;Период
			ИЛИ вогРейтинги.ДатаОкончания = ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0))
	И НЕ вогРейтинги.Закрыт
	И НЕ вогРейтинги.ПометкаУдаления
;


		<dataSource>БазаЗнаний</dataSource>
		<query>ВЫБРАТЬ
	CRM_ИспользуемыеРешения.Решение КАК Решение,
	CRM_ИспользуемыеРешения.Объект КАК Документ,
	ВЫБОР
		КОГДА CRM_ИспользуемыеРешения.Решение.Проект = ЗНАЧЕНИЕ(Справочник.Проекты.ПустаяСсылка)
			ТОГДА "[Проект не задан]"
		ИНАЧЕ CRM_ИспользуемыеРешения.Решение.Проект
	КОНЕЦ КАК Проект,
	CRM_ИспользуемыеРешения.Решение.СтатусРешения КАК Состояние,
	CRM_ИспользуемыеРешения.Пользователь КАК Пользователь,
	РешенияПользователя.Количество КАК КоличествоПользователя,
	РешенияВсего.Количество КАК КоличествоВсего,
	Решения.КоличествоРешений КАК КоличествоРешений
ИЗ
	РегистрСведений.CRM_ИспользуемыеРешения КАК CRM_ИспользуемыеРешения
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			CRM_ИспользуемыеРешения.Пользователь КАК Пользователь,
			CRM_ИспользуемыеРешения.Решение КАК Решение,
			СУММА(1) КАК Количество
		ИЗ
			РегистрСведений.CRM_ИспользуемыеРешения КАК CRM_ИспользуемыеРешения
		
		СГРУППИРОВАТЬ ПО
			CRM_ИспользуемыеРешения.Решение,
			CRM_ИспользуемыеРешения.Пользователь) КАК РешенияПользователя
		ПО CRM_ИспользуемыеРешения.Решение = РешенияПользователя.Решение
			И CRM_ИспользуемыеРешения.Пользователь = РешенияПользователя.Пользователь
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			CRM_ИспользуемыеРешения.Решение КАК Решение,
			СУММА(1) КАК Количество
		ИЗ
			РегистрСведений.CRM_ИспользуемыеРешения КАК CRM_ИспользуемыеРешения
		
		СГРУППИРОВАТЬ ПО
			CRM_ИспользуемыеРешения.Решение) КАК РешенияВсего
		ПО CRM_ИспользуемыеРешения.Решение = РешенияВсего.Решение,
	(ВЫБРАТЬ
		КОЛИЧЕСТВО(РАЗЛИЧНЫЕ CRM_Решения.Ссылка) КАК КоличествоРешений
	ИЗ
		Справочник.CRM_Решения КАК CRM_Решения
	ГДЕ
		НЕ CRM_Решения.ЭтоГруппа
		И НЕ CRM_Решения.ПометкаУдаления) КАК Решения
ГДЕ
	НЕ CRM_ИспользуемыеРешения.Решение.ЭтоГруппа
	И НЕ CRM_ИспользуемыеРешения.Решение.ПометкаУдаления

СГРУППИРОВАТЬ ПО
	CRM_ИспользуемыеРешения.Решение,
	CRM_ИспользуемыеРешения.Объект,
	CRM_ИспользуемыеРешения.Решение.СтатусРешения,
	РешенияПользователя.Количество,
	РешенияВсего.Количество,
	Решения.КоличествоРешений,
	CRM_ИспользуемыеРешения.Пользователь,
	ВЫБОР
		КОГДА CRM_ИспользуемыеРешения.Решение.Проект = ЗНАЧЕНИЕ(Справочник.Проекты.ПустаяСсылка)
			ТОГДА "[не задан]"
		ИНАЧЕ CRM_ИспользуемыеРешения.Решение.Проект
	КОНЕЦ</query>

			ПараметрыФормы.Вставить("ПараметрыНового",Параметры.ПараметрыНового);
			ОткрытьФорму("Справочник.ФизическиеЛица.ФормаОбъекта",ПараметрыФормы,Элементы.Список);
		КонецЕсли;
	КонецЕсли;
	
